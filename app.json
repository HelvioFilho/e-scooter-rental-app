{
  "expo": {
    "name": "E-Scooter",
    "slug": "e-scooter-rental",
    "version": "1.0.0",
    "scheme": "e-scooter-rental",
    "orientation": "portrait",
    "icon": "./src/assets/main/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/main/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "##E7AB8F"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hsvf.escooterrental"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/main/adaptive-icon.png",
        "backgroundColor": "##E7AB8F"
      },
      "package": "com.hsvf.escooterrental"
    },
    "experiments": {
      "typedRoutes": true,

      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.ey-your-key-here"
        }
      ]
    ]
  }
}
